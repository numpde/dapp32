{
  "elements": [
    {
      "type": "input",
      "label": "Enter your name",
      "id": "nameInput",
      "placeholder": "Your name here"
    },
    {
      "type": "select",
      "label": "Choose your favorite color",
      "id": "colorSelect",
      "options": [
        "Red",
        "Blue",
        "Green"
      ]
    },
    {
      "type": "button",
      "label": "Submit",
      "id": "submitButton",
      "onClick": {
        "function": {
          "name": "getSecondView",
          "inputs": [
            {
              "name": "user",
              "type": "address"
            }
          ],
          "outputs": [
            {
              "name": "",
              "type": "string"
            }
          ],
          "stateMutability": "pure"
        }
      }
    }
  ]
}
